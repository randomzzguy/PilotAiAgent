{
  "name": "linkedin-automation-system",
  "version": "1.0.0",
  "description": "Automated LinkedIn content generation and posting system for Abu Dhabi market",
  "main": "src/index.js",
  "scripts": {
    "start": "node src/index.js",
    "dev": "nodemon src/index.js",
    "test": "jest",
    "test:setup": "jest test/setup.test.js",
    "build": "npm run build:client",
    "build:client": "cd client && npm run build",
    "install:all": "npm install && cd client && npm install",
    "db:init": "node src/database/init.js",
    "worker:scheduler": "node src/workers/scheduler.js",
    "worker:analytics": "node src/workers/analytics.js",
    "setup": "node scripts/dev-setup.js",
    "deploy": "node scripts/deploy.js",
    "monitor": "node scripts/monitor.js",
    "monitor:watch": "node scripts/monitor.js watch",
    "backup": "node scripts/backup.js",
    "backup:restore": "node scripts/backup.js restore",
    "backup:list": "node scripts/backup.js list",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "docker:logs": "docker-compose logs -f",
    "docker:build": "docker-compose build",
    "pm2:start": "pm2 start ecosystem.config.js",
    "pm2:stop": "pm2 stop ecosystem.config.js",
    "pm2:restart": "pm2 restart ecosystem.config.js",
    "pm2:logs": "pm2 logs",
    "pm2:status": "pm2 status"
  },
  "keywords": [
    "linkedin",
    "automation",
    "social-media",
    "ai",
    "content-generation",
    "abu-dhabi"
  ],
  "author": "ENG. Sherif",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "axios": "^1.6.2",
    "node-cron": "^3.0.3",
    "moment-timezone": "^0.5.43",
    "openai": "^4.20.1",
    "pg": "^8.11.3",
    "bcryptjs": "^2.4.3",
    "jsonwebtoken": "^9.0.2",
    "multer": "^1.4.5-lts.1",
    "sharp": "^0.32.6",
    "helmet": "^7.1.0",
    "express-rate-limit": "^7.1.5",
    "winston": "^3.11.0",
    "joi": "^17.11.0",
    "uuid": "^9.0.1",
    "archiver": "^6.0.1",
    "extract-zip": "^2.0.1",
    "redis": "^4.6.10",
    "ioredis": "^5.3.2"
  },
  "devDependencies": {
    "nodemon": "^3.0.2",
    "jest": "^29.7.0",
    "supertest": "^6.3.3",
    "@types/node": "^20.10.4"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  }
}